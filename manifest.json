{
  "name": "Запускаем снежинки на любом сайте",
  "description": "Проект журнала Код",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "/icons/16.png",
    "32": "/icons/32.png",
    "64": "/icons/64.png",
    "128": "/icons/128.png",
    "256": "/icons/256.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "/icons/256.png"
  },
  "permissions": ["activeTab", "scripting"]
}